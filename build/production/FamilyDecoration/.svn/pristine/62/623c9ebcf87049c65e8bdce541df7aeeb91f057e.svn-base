    padding: 10px;
}

table.pros td{
    padding: 3px 5px;
    white-space:nowrap;
}

a#logout:link {
 color: #675c4c;
 font-family: 黑体;
 text-decoration: none
}
a#logout:visited {
 color: #675c4c;
 font-family: 黑体;
 text-decoration: none
}
a#logout:hover {
 color: #675c4c;
 font-family: 黑体;
 text-decoration: underline;
}
a#logout:active {
 color: #675c4c;

<?php
	include_once "conn.php";
	include_once "logListDb.php";
	$action = $_REQUEST["action"];
	$res = "";
	switch($action){
		case "addLogList": 	$res = addLogList($_REQUEST);  break;
		case "deleteLogList":$res = deleteLogList($_GET["logListId"]);break;
		case "deleteLogDetail":$res = deleteLogDetail($_GET["logDetailId"]);break;
		case "addLogDetail":$res = addLogDetail($_POST);break;
		case "getLogListYears":$res = getLogListYears();break;
		default: throw new Exception("unknown action:".$action);
	}
	echo urldecode(json_encode($res));
?>